{
  "name": "badma",
  "version": "0.1.0-alpha.38",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run unbuild; NODE_OPTIONS=\"--experimental-vm-modules\" jest --verbose --runInBand",
    "dev": "NODE_OPTIONS=\"--experimental-vm-modules\" npx -y hasyx dev",
    "client": "npx hasyx client",
    "build": "NODE_ENV=production npx -y hasyx build",
    "unbuild": "npx -y hasyx unbuild",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--experimental-vm-modules\" npx -y hasyx start",
    "ws": "npx --yes next-ws-cli@latest patch -y",
    "postinstall": "npm run ws -- -y",
    "migrate": "npx hasyx migrate",
    "unmigrate": "npx hasyx unmigrate",
    "npm-publish": "npm run build && npm publish",
    "events": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx events",
    "schema": "npx hasyx schema",
    "build:android": "npm run init:android && npm run client && npm run cli -- assets && npx cap sync android",
    "open:android": "npx cap open android",
    "build:ios": "npm run init:ios && npm run client && npm run cli -- assets && npx cap sync ios",
    "open:ios": "npx cap open ios",
    "doc:build": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx doc",
    "cli": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx",
    "assist": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx assist",
    "js": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx js",
    "tsx": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx tsx",
    "logs": "npx hasyx logs",
    "logs-diffs": "npx hasyx logs-diffs",
    "logs-states": "npx hasyx logs-states",
    "telegram": "NODE_OPTIONS=\"--experimental-vm-modules\" npx hasyx telegram",
    "config": "npx hasyx config",
    "env": "npx hasyx env",
    "gitpod": "npx hasyx gitpod",
    "build:css": "tailwindcss -i ./app/globals.css -o ./lib/styles.css",
    "init:android": "node -e \"process.exit(require('fs').existsSync('android')?0:1)\" || npx cap add android",
    "init:ios": "node -e \"process.exit(require('fs').existsSync('ios/App')?0:1)\" || npx cap add ios",
    "vercel": "npx hasyx vercel",
    "github": "npx hasyx github"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivansglazunov/badma.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivansglazunov/badma/issues"
  },
  "homepage": "https://github.com/ivansglazunov/badma#readme",
  "description": "A brilliant badma project - turning ideas into reality",
  "dependencies": {
    "chess.js": "^0.12.1",
    "cytoscape-grid-guide": "^2.3.3",
    "eruda": "^3.4.3",
    "events": "^3.3.0",
    "hasyx": "^0.2.0-alpha.45",
    "js-chess-engine": "^1.0.2",
    "motion": "^12.23.9",
    "react": "19.1.1",
    "react-chessboard": "^4.7.3",
    "react-confetti-boom": "^2.0.1",
    "react-dom": "19.1.1",
    "react-explode": "^2.0.5",
    "react-resize-detector": "^12.0.2",
    "uuid": "^11.1.0",
    "vercel": "^41.7.3",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chess.js": "^0.13.7",
    "@types/debug": "^4.1.12",
    "@types/jest": "29.5.12",
    "@types/node": "^20.11.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "bcrypt": "^5.1.1",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "typescript": "^5.3.3"
  },
  "engine": {
    "node": "^22.14"
  },
  "overrides": {
    "zod": "^4.0.15",
    "hasyx": {
      "react": "19.1.1",
      "react-dom": "19.1.1"
    }
  }
}
